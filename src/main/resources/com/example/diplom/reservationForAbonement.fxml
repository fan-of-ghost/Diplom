         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="172.0" prefHeight="60.0" prefWidth="396.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Выберите дату" wrappingWidth="200.0" />
