<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.diplom.Controllers.ReservationForCertificateController"
            prefHeight="287.0" prefWidth="415.0">
    <children>
        <HBox layoutX="10.0" layoutY="49.0" prefHeight="60.0" prefWidth="396.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Выберите абонемент" wrappingWidth="200.0" />
                <ComboBox fx:id="comboboxIdCertificate" prefHeight="26.0" prefWidth="180.0" />
            </children>
        </HBox>
        <HBox layoutX="10.0" layoutY="109.0" prefHeight="60.0" prefWidth="396.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Выберите, сколько минут клиент хочет потратить из оставшихся" wrappingWidth="200.0" />
                <Spinner fx:id="spinnerMinutes" prefHeight="26.0" prefWidth="180.0" />
            </children>
        </HBox>
        <HBox layoutX="10.0" layoutY="172.0" prefHeight="60.0" prefWidth="396.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Выберите дату" wrappingWidth="200.0" />
                <DatePicker fx:id="dateOfReservation" editable="false" prefHeight="26.0" prefWidth="180.0" />
            </children>
        </HBox>
        <Button layoutX="171.0" layoutY="232.0" mnemonicParsing="false" onAction="#reserveButton" text="Сохранить" />
    </children>
</AnchorPane>
