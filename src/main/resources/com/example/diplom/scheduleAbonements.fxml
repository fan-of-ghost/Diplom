<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="393.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.diplom.Controllers.ScheduleAbonementsController">

    <VBox>
        <children>
            <HBox alignment="CENTER" spacing="10">
                <children>
                    <Label text="Год:" />
                    <ComboBox fx:id="yearComboBox" />
                    <Label text="Месяц:" />
                    <ComboBox fx:id="monthComboBox" />
                </children>
            </HBox>
            <GridPane fx:id="calendarGrid" hgap="10" vgap="10">
                <padding>
                    <Insets bottom="10" left="10" right="10" top="10" />
                </padding>
            </GridPane>
        </children>
    </VBox>
</AnchorPane>
