<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.diplom.Controllers.MainAdminAbonementsController">
   <children>
      <TableView fx:id="tableViewAbonements" layoutY="74.0" prefHeight="526.0" prefWidth="799.0">
        <columns>
            <TableColumn fx:id="idAbonement" prefWidth="52.800018310546875" resizable="false" text="№">
                <cellValueFactory>
                    <PropertyValueFactory property="id" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="nominalAbonement" prefWidth="121.60000610351562" resizable="false" text="Номинал (мин)">
                <cellValueFactory>
                    <PropertyValueFactory property="nominal" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="dateOfUseAbonement" prefWidth="96.0" resizable="false" text="Д/a исп">
                <cellValueFactory>
                    <PropertyValueFactory property="dateOfUse" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="balanceAbonement" prefWidth="98.4000244140625" resizable="false" text="Остаток (мин)">
                <cellValueFactory>
                    <PropertyValueFactory property="balance" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="dateOfBuyAbonement" prefWidth="85.5999755859375" resizable="false" text="Д/a покупки">
                <cellValueFactory>
                    <PropertyValueFactory property="dateOfBuy" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="dateOfEndAbonement" prefWidth="101.60003662109375" resizable="false" text="Д/a истечения">
                <cellValueFactory>
                    <PropertyValueFactory property="dateOfEnd" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="dateOfResAbonement" prefWidth="102.4000244140625" resizable="false" text="Д/a продления">
                <cellValueFactory>
                    <PropertyValueFactory property="dateOfRes" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="statusAbonement" prefWidth="49.60009765625" resizable="false" text="Статус">
                <cellValueFactory>
                    <PropertyValueFactory property="status" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn fx:id="idClientAbonement" prefWidth="85.60002441406255" resizable="false" text="№ клиента">
                <cellValueFactory>
                    <PropertyValueFactory property="idClient" />
                </cellValueFactory>
            </TableColumn>

        </columns>
      </TableView>
      <VBox layoutY="1.0" prefHeight="27.0" prefWidth="226.0">
         <children>
            <MenuBar prefHeight="26.0" prefWidth="726.0">
              <menus>
                <Menu mnemonicParsing="false" text="Файл">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Сохранить" onAction="#onSaveToFileClick"/>
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Редактирование">
                  <items>
                    <MenuItem fx:id="addNewAbonement" mnemonicParsing="false" onAction="#onAddClick" text="Добавить" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
